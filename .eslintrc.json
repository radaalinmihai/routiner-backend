{
	"env": {
		"browser": true,
		"es2021": true
	},
	"extends": ["plugin:@typescript-eslint/recommended", "plugin:prettier/recommended"],
	"parser": "@typescript-eslint/parser",
	"parserOptions": {
		"ecmaVersion": 9,
		"sourceType": "module",
		"project": "./tsconfig.json"
	},
	"plugins": ["@typescript-eslint"],
	"rules": {
		"prettier/prettier": ["error"]
	}
}
